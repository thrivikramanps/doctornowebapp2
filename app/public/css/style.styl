body
  margin 0
  padding 0
  height 100%
  background #f2f9fe
  background -moz-radial-gradient(center, ellipse cover, #f2f9fe 0%, #d6f0fd 100%)
  background -webkit-gradient(radial, center center, 0px, center center, 100%, color-stop(0%, #f2f9fe), color-stop(100%, #d6f0fd))
  background -webkit-radial-gradient(center, ellipse cover, #f2f9fe 0%, #d6f0fd 100%)
  background -o-radial-gradient(center, ellipse cover, #f2f9fe 0%, #d6f0fd 100%)
  background -ms-radial-gradient(center, ellipse cover, #f2f9fe 0%, #d6f0fd 100%)
  background radial-gradient(center, ellipse cover, #f2f9fe 0%, #d6f0fd 100%)

#header-container
  width 980px
  margin 20px auto

#middle-container
  width 980px
  margin 20px auto

#login-container
  width 340px
  margin 0px
  label
    margin 15px 0 5px 5px
  button
    width 150px
    padding 6px 40px 6px 40px
    float left
    margin-top 15px
  .checkbox
    margin 23px 0 0 180px
  .btm-links
    text-align center
    margin-top 10px
  #create-account
    float right
    margin-right 13px
  #forgot-password
    float left
    margin-left 10px

#photo-container
  width 640px
  margin 0px 0px 0px 360px

#footer-container
  width 980px
  margin 30px auto

#account-form-container
  width 460px
  margin 100px auto

#account-dashboard-container
  width 1000px
  margin 100px auto

#button-panel
  button
    width 150px
    padding 6px 40px 6px 40px
    float left
    margin-top 15px

#account-dashboard
  button
    width 250px
    height 300px
    padding 6px 40px 6px 40px
    float center
    margin-top 15px
    margin-left 50px

#state-list
  width 280px

.modal-form-errors
  li
    color #808080
    text-shadow -1px 1px 0 #fff
    -moz-text-shadow -1px 1px 0 #fff
    -webkit-text-shadow -1px 1px 0 #fff
    color #d8000c

.modal-single-input
  width 452px
  margin -200px 0 0 -232px
  form
    color #808080
    text-shadow -1px 1px 0 #fff
    -moz-text-shadow -1px 1px 0 #fff
    -webkit-text-shadow -1px 1px 0 #fff
    margin-bottom 0
    button
      width 80px
      top 213px
      position absolute
    #cancel
      right 110px
    #submit
      right 18px
  .modal-body
    height 140px
    overflow hidden
    padding-bottom 0
  .modal-footer
    height 20px

.modal-alert
  width 310px
  margin -200px 0 0 -150px

#btn-logout
  width 100px
  top 1px
  right 8px
  position absolute

#print
  padding 20px

#four04
  padding 20px

.clear-fix
  clear both

.well
  margin-left 0
  border 1px solid #ccc

.form-horizontal
  .control-label
    width 80px
  .controls
    margin-left 120px
  .form-actions
    padding 15px 0 0 270px

label
  color #808080
  text-shadow -1px 1px 0 #fff
  -moz-text-shadow -1px 1px 0 #fff
  -webkit-text-shadow -1px 1px 0 #fff

.subheading
  color #808080
  text-shadow -1px 1px 0 #fff
  -moz-text-shadow -1px 1px 0 #fff
  -webkit-text-shadow -1px 1px 0 #fff
  margin-top 3px

button
  i
    padding-left 8px

